<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Wordpress - LogInTest" verbose="1">

    <test name="PositiveLogInTest">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="apps.web.Wordpress.TestPositiveLogIn"/>
        </classes>
    </test>

    <test name="NegativeLogInTest">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="apps.web.Wordpress.TestNegativeLogIn"/>
        </classes>
    </test>

</suite>